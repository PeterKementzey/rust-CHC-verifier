(set-logic HORN)
(set-option :fp.engine spacer)
(set-option :model true)

(declare-fun q1 (Int) Bool)
(declare-fun q2 (Int Int Int) Bool)
(declare-fun q3 (Int Int Int Int) Bool)
(declare-fun q4 (Int Int) Bool)
(declare-fun q5 (Int Int Int Int) Bool)
(declare-fun q6 (Int Int Int) Bool)
(declare-fun q7 (Int Int) Bool)
(declare-fun q8 () Bool)
(assert (forall ((|x| Int)) (=> (and (= |x| 42)) (q1 |x|))))
(assert (forall ((|x| Int) (|x'| Int) (|y*| Int) (|y^| Int)) (=> (and (q1 |x|) (= |y*| |x|) (= |x'| |y^|)) (q2 |x'| |y*| |y^|))))
(assert (forall ((|a| Int) (|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q2 |x| |y*| |y^|) (= |a| 0)) (q3 |x| |y*| |y^| |a|))))
(assert (forall ((|a| Int) (|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q3 |x| |y*| |y^| |a|)) (= |y*| 42))))
(assert (forall ((|a| Int) (|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q3 |x| |y*| |y^| |a|) (= |y^| |y*|)) (q4 |x| |a|))))
(assert (forall ((|a| Int) (|a'| Int) (|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q4 |x| |a|) (= |y*| |a|) (= |a'| |y^|)) (q5 |x| |a'| |y*| |y^|))))
(assert (forall ((|a| Int) (|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q5 |x| |a| |y*| |y^|)) (q6 |x| |y*| |y^|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q6 |x| |y*| |y^|)) (= |x| 42))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q6 |x| |y*| |y^|)) (q7 |y*| |y^|))))
(assert (forall ((|y*| Int) (|y^| Int)) (=> (and (q7 |y*| |y^|)) (= |y*| 0))))
(assert (forall ((|y*| Int) (|y^| Int)) (=> (and (q7 |y*| |y^|) (= |y^| |y*|)) q8)))

(check-sat)
(get-model)
