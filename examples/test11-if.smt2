(set-logic HORN)
(set-option :fp.engine spacer)
(set-option :model true)

(declare-fun q1 (Int) Bool)
(declare-fun q2 (Int Int Int) Bool)
(declare-fun q3 (Int Int Int Int) Bool)
(declare-fun q4 (Int Int Int Int) Bool)
(declare-fun q5 (Int Int Int Int) Bool)
(declare-fun q6 (Int Int) Bool)
(declare-fun q7 (Int) Bool)
(declare-fun q8 (Int Int) Bool)
(declare-fun q9 (Int) Bool)
(declare-fun q10 (Int) Bool)
(declare-fun q11 (Int Int Int Int) Bool)
(declare-fun q12 (Int Int Int) Bool)
(assert (forall ((|x| Int)) (=> (and (= |x| 42)) (q1 |x|))))
(assert (forall ((|x| Int) (|x'| Int) (|y*| Int) (|y^| Int)) (=> (and (q1 |x|) (= |y*| |x|) (= |x'| |y^|)) (q2 |x'| |y*| |y^|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q2 |x| |y*| |y^|) (= |z| 42)) (q3 |x| |y*| |y^| |z|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q3 |x| |y*| |y^| |z|) (= |y*| |z|)) (q4 |x| |y*| |y^| |z|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q4 |x| |y*| |y^| |z|) (= |y^| |y*|)) (q6 |x| |z|))))
(assert (forall ((|x| Int) (|z| Int)) (=> (and (q6 |x| |z|)) (q7 |x|))))
(assert (forall ((|a| Int) (|x| Int)) (=> (and (q7 |x|) (= |a| 3)) (q8 |x| |a|))))
(assert (forall ((|a| Int) (|x| Int)) (=> (and (q8 |x| |a|)) (q9 |x|))))
(assert (forall ((|x| Int) (|x'| Int)) (=> (and (q9 |x|) (= |x'| 1)) (q10 |x'|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q3 |x| |y*| |y^| |z|) (not (= |y*| |z|))) (q5 |x| |y*| |y^| |z|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q10 |x|)) (q11 |x| |y*| |y^| |z|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q5 |x| |y*| |y^| |z|)) (q11 |x| |y*| |y^| |z|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q11 |x| |y*| |y^| |z|)) (= |x| 1))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q11 |x| |y*| |y^| |z|)) (distinct |x| 2))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int) (|z| Int)) (=> (and (q11 |x| |y*| |y^| |z|)) (q12 |y*| |y^| |z|))))

(check-sat)
(get-model)
