(set-logic HORN)
(set-option :fp.engine spacer)
(set-option :model true)

(declare-fun q1 (Int) Bool)
(declare-fun q2 (Int Int Int) Bool)
(declare-fun q3 (Int Int Int) Bool)
(declare-fun q4 (Int Int Int) Bool)
(declare-fun q5 (Int Int Int) Bool)
(declare-fun q6 (Int Int Int) Bool)
(declare-fun q7 (Int) Bool)
(declare-fun q8 (Int) Bool)
(declare-fun q9 (Int) Bool)
(declare-fun q10 () Bool)
(assert (forall ((|x| Int)) (=> (and (= |x| 0)) (q1 |x|))))
(assert (forall ((|x| Int) (|x'| Int) (|y*| Int) (|y^| Int)) (=> (and (q1 |x|) (= |y*| |x|) (= |x'| |y^|)) (q2 |x'| |y*| |y^|))))
(assert (forall ((|x| Int) (|y'*| Int) (|y*| Int) (|y^| Int)) (=> (and (q2 |x| |y*| |y^|) (= |y'*| 2)) (q3 |x| |y'*| |y^|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q3 |x| |y*| |y^|) (= |y*| 2)) (q4 |x| |y*| |y^|))))
(assert (forall ((|x| Int) (|y'*| Int) (|y*| Int) (|y^| Int)) (=> (and (q4 |x| |y*| |y^|) (= |y'*| 1)) (q6 |x| |y'*| |y^|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q6 |x| |y*| |y^|) (= |y^| |y*|)) (q7 |x|))))
(assert (forall ((|x| Int)) (=> (and (q7 |x|)) (= |x| 1))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q3 |x| |y*| |y^|) (not (= |y*| 2))) (q5 |x| |y*| |y^|))))
(assert (forall ((|x| Int) (|y*| Int) (|y^| Int)) (=> (and (q5 |x| |y*| |y^|) (= |y^| |y*|)) (q8 |x|))))
(assert (forall ((|x| Int) (|x'| Int)) (=> (and (q8 |x|) (= |x'| 2)) (q9 |x'|))))
(assert (forall ((|x| Int)) (=> (and (q7 |x|)) (q9 |x|))))
(assert (forall ((|x| Int)) (=> (and (q9 |x|)) (= |x| 1))))
(assert (forall ((|x| Int)) (=> (and (q9 |x|)) (distinct |x| 2))))
(assert (forall ((|x| Int)) (=> (and (q9 |x|)) q10)))

(check-sat)
(get-model)
